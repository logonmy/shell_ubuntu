<script type="text/javascript">
	$(window).load(function(){
		function GameHelp() {}
		GameHelp.$ = $("#game-help");
		GameHelp.timer = null;
		GameHelp.v = 1000;
		GameHelp.animate = function(gh) {
			gh.$.toggleClass("act");
			gh.timer = setTimeout(function() {
				gh.animate(gh);
			}, gh.v);
		}
		GameHelp.init = function(gh) {
			gh.animate(gh);
			gh.$.hover(
				function() { clearTimeout(gh.timer); },
				function(){ gh.animate(gh); }
			);
		}
		
		GameHelp.init(GameHelp);
	});
</script>
