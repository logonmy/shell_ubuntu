                    $(".index_all_games tr:odd").addClass("white_bg");
                    $(".index_all_games tr:even").addClass("gray_bg");
                    
                     var showTimer, hideTimer;
                      $(".index_all_games tr").mouseenter(function(){
                        clearTimeout(showTimer);
                        var _this=$(this);
                        showTimer=setTimeout(function(){
                            _this.css({"height":66}).find("img,p").fadeIn(100);
							_this.find('img').attr('src', function(){
								var self = $(this);
								if(!self.attr('src')) self.attr('src', self.attr('_src'));
							});
                            _this.siblings("tr").css({"height":33}).find("img,p").fadeOut(100);
                        },50)
                    });
                    $(".index_all_games tr").mouseleave(function(){
                        clearTimeout(showTimer);
                    });
                
                    $(".index_all_games tr:eq(0)").css({"height":66}).find("img,p").show();
					$(".index_all_games tr:eq(0)").find('img').attr('src', function(){
						var self = $(this);
						if(!self.attr('src')) self.attr('src', self.attr('_src'));
					});
                    $(".index_all_games td:nth-child(2n)").css("border-right","none");
