					html += '<td width="34%" align="center"><a href="http://wvw.9377.com/game_login.php?game=lt2&server=272" target="_blank">双线272区</a></td>';
					html += '<td width="20%" align="center"><a class="status_btn" href="http://wvw.9377.com/game_login.php?game=lt2&server=272" target="_blank">立即进入</a></td>';
					html += '</tr>';
					html += '';
					html += '</tbody>';
					$('#last_games').html(html);
				}

				$(function(){
					$('.friends_link').remove();
					$('.sign_in').live('click', function(){
						$.getJSON('http://wvw.9377.com/api/user_sign_in.php?callback=?', function(res){
							if( res == 0 ) {
								alert('请先登录后再签到。');
							}else if( res == -1 ) {
								alert('对不起，您今天已签到，明天再来吧。');
							}else if( res > 0 ) {
								alert('签到成功，获得'+res+'积分。');
								$('.user_credit').html(parseInt($('.user_credit').html())+res);
								$('.up_credits').html(parseInt($('.up_credits').html())+res);
							}else {
								alert('对不起，网络繁忙，请稍候重试。');
							}
						});
					});
				});
			});
		</script>
	</div>
