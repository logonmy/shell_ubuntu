	<div class="login_main">
		<script type="text/javascript">
		function _login_form_index(form){
			var form = $(form);
			var username = form.find('input[name=username]').val();
			if(username.length < 3 || username.length > 20) {
				//alert("您输入的帐号有误");
				$('.log_tip').css('top', '-20px').show();
				$('.log_tip_content').html('您输入的帐号有误');
				form.find('input[name=username]').focus();
				setTimeout(function(){$('.log_tip').fadeOut();},2000);
				return false;
			}
			
			var pwd = form.find('input[name=password]').val();
			if(pwd.length < 1) {
				//alert("您输入的密码不正确");
				$('.log_tip').css('top', '30px').show();
				$('.log_tip_content').html('您输入的密码不正确');
				form.find('input[name=password]').focus();
				setTimeout(function(){$('.log_tip').fadeOut();},2000);
				return false;
			}
			
			return true;
		}
		</script>
