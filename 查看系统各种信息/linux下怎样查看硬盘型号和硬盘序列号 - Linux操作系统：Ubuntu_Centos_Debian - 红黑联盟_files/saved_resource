            <script type="text/javascript">
                $(function(){
                    $(".hot_game li .site_pic").hover(function() {
						if (!$(this).children(".site_pic_back").is(":animated")) {
                        $(this).find(".site_pic_back").fadeIn();
						return false;
						}
                    }, function() {
                        $(this).find(".site_pic_back").fadeOut();
						return false;
                    });
					//开始游戏按钮
					var games_time = null;
					$('.games_start_tit span').mouseover(function(){
						clearTimeout(games_time);
						$('.games_start_tit span').addClass('start_up');
						$('.games_start_con').show();
					}).mouseout(function(){
						games_time = setTimeout(function(){
							$('.games_start_tit span').removeClass('start_up');
							$('.games_start_con').hide();
						},300);
					});
					
					$('.games_start_con').mouseover(function(){
						clearTimeout(games_time);
						$('.games_start_tit span').addClass('start_up');
						$('.games_start_con').show();
					}).mouseout(function(){
						$('.games_start_con').hide();
						$('.games_start_tit span').removeClass('start_up');
					});
                });
            </script>
